<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="158.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Deposite">
   <children>
      <AnchorPane layoutY="-6.0" prefHeight="169.0" prefWidth="600.0" style="-fx-background-color: #0cbfda;">
         <children>
            <Label layoutX="60.0" layoutY="54.0" text="Account Number :" textFill="#f50cbb">
               <font>
                  <Font name="DejaVu Sans Bold Oblique" size="14.0" />
               </font>
            </Label>
            <Label layoutX="58.0" layoutY="102.0" text="Deposite Amount :" textFill="#dd0bc8">
               <font>
                  <Font name="DejaVu Sans Bold Oblique" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="accNumber" layoutX="231.0" layoutY="51.0" prefHeight="26.0" prefWidth="174.0" promptText="Type your account number">
               <font>
                  <Font name="DejaVu Sans Bold Oblique" size="10.0" />
               </font></TextField>
            <TextField fx:id="depositeAmount" layoutX="231.0" layoutY="99.0" prefHeight="26.0" prefWidth="174.0" promptText="Type your deposite amount">
               <font>
                  <Font name="DejaVu Sans Bold Oblique" size="10.0" />
               </font></TextField>
            <Button layoutX="490.0" layoutY="97.0" mnemonicParsing="false" onAction="#EnterButton" text="Enter" textFill="#036f13">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button layoutX="490.0" layoutY="49.0" mnemonicParsing="false" onAction="#ExitButton" prefHeight="30.0" prefWidth="52.0" text="Exit" textFill="#e10808">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
